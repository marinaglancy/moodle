{{!
    @template mod_feedback/itemdependencies

    Displays one item for editing

    Classes required for JS:
    * none

    Data attributes required for JS:
    * none

    Context variables required for this template:
    * none

    Example context (json):
    {
        "dependencies": [{
                "dependencylabel": "wronglabel",
                "dependencyvalue": "wrongvalue",
                "dependencyerror": "This dependency has an error"
            }, {
                "dependencylabel": "correctlabel",
                "dependencyvalue": "correctvalue"
            }
        ]
    }

}}

{{#dependencies}}
    <span class="feedback_depend {{#dependencyerror}}feedback_depend_invalid{{/dependencyerror}}">
        ({{#dependencyerror}}{{#pix}} i/warning, moodle, {{dependencyerror}}
    {{/pix}}{{/dependencyerror}}{{{dependencylabel}}}-&gt;{{{dependencyvalue}}})
    </span>
{{/dependencies}}
